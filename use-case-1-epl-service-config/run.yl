#!/bin/bash
# yanglint interactive scripting
JSON_FILE=mpi1-epl-service-config.json
YANG_FILE=ietf-eth-tran-service@2018-03-01.yang 
#YANG_AUG_FILE=ietf-otn-tunnel@2018-06-07.yang

JSON_FILE_TMP=tmp-${JSON_FILE}
cp $JSON_FILE $JSON_FILE_TMP

python ../../json-yang/scripts/Stripper.py -i $JSON_FILE_TMP --clean

echo "add -i ${YANG_FILE}
      add -i ${YANG_FILE}
      data ${JSON_FILE_TMP}" | yanglint

rm $JSON_FILE_TMP
